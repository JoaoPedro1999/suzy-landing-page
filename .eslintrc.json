{
  "extends": [
    "next/core-web-vitals",
    "@rocketseat/eslint-config/react"
  ],
  "rules": {
    "import/order": [
      "error",
      {
        "groups": [
          "external",
          "internal",
          "parent",
          "sibling",
          "builtin",
          "object",
          "type",
          "index"
        ],
        "newlines-between": "always-and-inside-groups"
      }
    ]
  }
}